{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-dongtai",
      "type": "shell",
      "command": "C:\\msys64\\ucrt64\\bin\\gcc.exe",
      "args": [
        "-g",
        "${workspaceFolder}/Test/dongtai list.c",
        "-o",
        "${workspaceFolder}/Test/dongtai list.exe"
      ],
      "options": {
        "cwd": "${workspaceFolder}/Test"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": false
      }
    },
    {
      "label": "build-insert",
      "type": "shell",
      "command": "C:\\msys64\\ucrt64\\bin\\g++.exe",
      "args": [
        "-g",
        "${workspaceFolder}/Test/insert list.cpp",
        "-o",
        "${workspaceFolder}/Test/insert list.exe"
      ],
      "options": {
        "cwd": "${workspaceFolder}/Test"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": false
      }
    },
    {
      "type": "cppbuild",
      "label": "C/C++: gcc.exe 生成活动文件",
      "command": "C:\\msys64\\ucrt64\\bin\\gcc.exe",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${file}",
        "-o",
        "${fileDirname}\\${fileBasenameNoExtension}.exe"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "调试器生成的任务。"
    }
  ]
}